{
  "name": "@brettz9/git-utilities",
  "version": "1.4.0",
  "description": "CLI utility scripts for Git",
  "main": "index.js",
  "bin": {
    "open-git-url": "./src/open-git-url.js"
  },
  "scripts": {
    "eslint": "eslint --ext js,md,html .",
    "build-cli-html": "clp --format=html --config=\"src/optionDefinitions.js\" \"cli.html\"",
    "build-cli-svg": "clp --format=svg --config=\"src/optionDefinitions.js\" \"cli.svg\"",
    "example-shell": "./bin/open-git-url.sh $INIT_CWD/.eslintrc.js",
    "example-js": "./src/open-git-url.js $INIT_CWD/.eslintrc.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Brett Zamir",
  "contributors": [],
  "license": "MIT",
  "keywords": [
    "git"
  ],
  "bugs": "https://github.com/brettz9/git-utilities/issues",
  "homepage": "https://github.com/brettz9/git-utilities",
  "engines": {
    "node": ">=10.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brettz9/git-utilities"
  },
  "dependencies": {
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0",
    "dialog-node": "^0.2.1",
